{
  "name": "aurelia-bs",
  "version": "0.9.17",
  "description": "A plugin for Aurelia providing lots of components for the bootstrap UI framework.",
  "keywords": [
    "aurelia",
    "plugin",
    "bootstrap"
  ],
  "homepage": "https://github.com/RSuter/aurelia-bs",
  "bugs": {
    "url": "https://github.com/RSuter/aurelia-bs/issues"
  },
  "license": "MIT",
  "author": "Rico Suter",
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/RSuter/aurelia-bs/issues"
  },
  "scripts": {
    "lint": "cross-env tslint --project tsconfig.json",
    "prebuild:amd": "cross-env rimraf dist/amd",
    "build:amd": "cross-env tsc --project tsconfig.build.json --outDir dist/amd --module amd",
    "postbuild:amd": "cross-env copyfiles --up 1 src/**/*.html src/**/*.css dist/amd",
    "prebuild:commonjs": "cross-env rimraf dist/commonjs",
    "build:commonjs": "cross-env tsc --project tsconfig.build.json --outDir dist/commonjs --module commonjs",
    "postbuild:commonjs": "cross-env copyfiles --up 1 src/**/*.html src/**/*.css dist/commonjs",
    "prebuild:es2015": "cross-env rimraf dist/es2015",
    "build:es2015": "cross-env tsc --project tsconfig.build.json --outDir dist/es2015 --module es2015 --target es2015",
    "postbuild:es2015": "cross-env copyfiles --up 1 src/**/*.html src/**/*.css dist/es2015",
    "prebuild:native-modules": "cross-env rimraf dist/native-modules",
    "build:native-modules": "cross-env tsc --project tsconfig.build.json --outDir dist/native-modules --module es2015",
    "postbuild:native-modules": "cross-env copyfiles --up 1 src/**/*.html src/**/*.css dist/native-modules",
    "prebuild:system": "cross-env rimraf dist/system",
    "build:system": "cross-env tsc --project tsconfig.build.json --outDir dist/system --module system",
    "postbuild:system": "cross-env copyfiles --up 1 src/**/*.html src/**/*.css dist/system",
    "prebuild": "cross-env rimraf dist && rimraf node_modules/@types/!node",
    "build": "concurrently \"npm run build:amd\" \"npm run build:commonjs\" \"npm run build:es2015\" \"npm run build:native-modules\" \"npm run build:system\" \"au build --env prod\" \"if exist demo rmdir demo /s /q\" && xcopy \"dist/demo\" \"demo\" /e /i /y /s",
    "bump-version": "npm --no-git-tag-version version",
    "start": "cmd /c call \"./node_modules/.bin/au.cmd\" run"
  },
  "dependencies": {
    "@types/bootstrap-datepicker": "^0.0.11",
    "@types/jquery": "^3.2.12",
    "aurelia-dependency-injection": "^1.0.0",
    "aurelia-framework": "^1.0.0",
    "aurelia-metadata": "^1.0.0",
    "aurelia-pal": "^1.0.0",
    "aurelia-templating": "^1.0.0",
    "aurelia-validation": "^1.1.1",
    "bootstrap-datepicker": "^1.7.1",
    "jquery": "^3.2.1",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.47",
    "@types/lodash": "^4.14.69",
    "@types/node": "^6.0.45",
    "@types/webpack": "^3.0.4",
    "aurelia-animator-css": "^1.0.2",
    "aurelia-binding": "^1.2.1",
    "aurelia-bootstrapper": "^2.1.1",
    "aurelia-cli": "^0.31.3",
    "aurelia-event-aggregator": "^1.0.1",
    "aurelia-loader": "^1.0.0",
    "aurelia-loader-default": "^1.0.2",
    "aurelia-polyfills": "latest",
    "aurelia-templating-binding": "^1.3.0",
    "aurelia-testing": "^1.0.0-beta.3.0.1",
    "aurelia-tools": "^1.0.0",
    "aurelia-webpack-plugin": "^2.0.0-rc.4",
    "awesome-typescript-loader": "^3.2.1",
    "bluebird": "^3.5.1",
    "bootstrap": "^3.3.7",
    "concurrently": "^3.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "copy-webpack-plugin": "4.0.1",
    "copyfiles": "^1.0.0",
    "cross-env": "^3.1.0",
    "css-loader": "0.28.4",
    "del": "^3.0.0",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-rename": "^1.2.2",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.30.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "0.5.7",
    "minimatch": "^3.0.2",
    "nps-utils": "1.2.0",
    "opn": "^5.1.0",
    "requirejs": "^2.2.0",
    "requirejs-text": "^2.0.12",
    "rimraf": "^2.5.4",
    "style-loader": "0.18.2",
    "through2": "^2.0.1",
    "ts-node": "^3.2.0",
    "tslint": "^5.7.0",
    "typescript": ">=1.9.0-dev || ^2.0.0",
    "uglify-js": "^3.0.19",
    "url-loader": "0.5.8",
    "vinyl-fs": "^2.4.3",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1"
  },
  "aurelia": {
    "build": {
      "resources": [
        "aurelia-bs/resize-container",
        "aurelia-bs/dialogs/alert-dialog",
        "aurelia-bs/dialog-service",
        "aurelia-bs/dialog",
        "aurelia-bs/grid/column",
        "aurelia-bs/grid/grid",
        "aurelia-bs/select-grid/select-grid-dialog",
        "aurelia-bs/select-grid/select-grid",
        "aurelia-bs/tabs/tab",
        "aurelia-bs/tabs/tabs",
        "aurelia-bs/accordion/accordion",
        "aurelia-bs/accordion/accordion-item",
        "aurelia-bs/accordion/accordion-item-header",
        "aurelia-bs/button",
        "aurelia-bs/checkbox",
        "aurelia-bs/datepicker",
        "aurelia-bs/expander",
        "aurelia-bs/fileupload",
        "aurelia-bs/grid-filter",
        "aurelia-bs/label-collection",
        "aurelia-bs/loader",
        "aurelia-bs/navbar-header",
        "aurelia-bs/scroll",
        "aurelia-bs/searchbox",
        "aurelia-bs/select",
        "aurelia-bs/textbox",
        "aurelia-bs/events",
        "aurelia-bs/convert",
        "aurelia-bs/validation-component",
        "aurelia-bs/validation"
      ]
    }
  }
}